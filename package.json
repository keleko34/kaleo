{
  "name": "kaleo",
  "version": "0.0.0",
  "private": true,
  "main": "localhost:8080",
  "nodejs": true,
  "node-remote": "*://*",
  "single-instance": true,
  "window": {
    "frame": false,
    "toolbar": false,
    "width": 1280,
    "height": 720
  },
  "chromium-args": "--enable-gpu-rasterization --enable-gpu-memory-buffer-video-frame --enable-native-gpu-memory-buffers --enable-zero-copy --enable-gpu-async-worker-context",
  "scripts": {
    "start": "npm run vue-build && node ./Scripts/dist && npm run nw-serve",
    "debug": "node ./Scripts/debug",
    "build": "npm run vue-build && node ./Scripts/dist && npm run nw-build",
    "vue-serve": "vue-cli-service serve",
    "vue-build": "vue-cli-service build",
    "nw-serve": "node_modules\\nw\\nwjs\\nw ./Build --disable-raf-throttling",
    "nw-debug": "node_modules\\nw\\nwjs\\nw . --disable-raf-throttling --enable-logging --remote-debugging-port=1729 --enable-gpu-benchmarking --enable-gpu-client-logging --enable-gpu-debugging --load-extension=Scripts/Extensions/ShaderEditorExtension/src,node_modules/nw-vue-devtools/extension",
    "nw-build": "node ./Scripts/build",
    "postinstall": "npm run setup",
    "setup": "node ./Scripts/setup",
    "kill": "node ./Scripts/kill"
  },
  "dependencies": {
    "core-js": "2.6.5",
    "nw": "0.38.3-sdk",
    "vue": "2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.7.0",
    "@vue/cli-plugin-eslint": "3.7.0",
    "@vue/cli-service": "3.7.0",
    "@vue/eslint-config-standard": "4.0.0",
    "babel-eslint": "10.0.1",
    "chrome-launcher": "^0.10.7",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "5.0.0",
    "nw-builder": "3.5.7",
    "nw-vue-devtools": "^1.3.0",
    "ps-node": "0.1.6",
    "sass": "1.18.0",
    "sass-loader": "7.1.0",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/base",
      "@vue/standard"
    ],
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "semi": "off",
      "keyword-spacing": "off",
      "brace-style": "off",
      "one-var": "off",
      "no-trailing-spaces": "off",
      "space-before-function-paren": "off",
      "indent": "off",
      "semi-spacing": "off",
      "space-infix-ops": "off",
      "no-extra-parens": "off",
      "padded-blocks": "off",
      "valid-typeof": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@vue/app"
    ]
  }
}
